  "properties": {
    "additional_costs": {
      "type": "array",
      "title": "Additional costs associated with the primary quote",
      "items": {
        "type": "object",
        "title": "Additional cost",
        "required": [
          "id",
          "name",
          "description",
          "unit_type"
        ],
        "properties": {
          "id": {
            "type": "string",
            "title": "ID",
            "examples": [
              "fa985737-8172-471a-97ea-f2c87c67111b"
            ]
          },
          "name": {
            "type": "string",
            "title": "Name",
            "examples": [
              "A cost"
            ]
          },
          "description": {
            "type": "string",
            "title": "Description",
            "examples": [
              "Lorem ipsum"
            ]
          },
          "unit_type": {
            "type": "string",
            "title": "type cost",
            "enum": ["per_hour", "per_item"],
            "examples": [
              "per_hour",
              "per_item"
            ]
          },
          "cost_per_hour": {
            "type": ["string", "null"],
            "title": "Decimal representation of the hourly cost - only required for per_hour costs",
            "examples": [
              "10.3"
            ]
          },
          "cost_per_hour_original": {
            "type": ["string", "null"],
            "title": "Pre-adjustment hourly cost",
            "examples": [
              "10.3"
            ]
          },
          "cost_per_item": {
            "type": ["string", "null"],
            "title": "Decimal representation of the per-item cost - only required for per_item costs",
            "examples": [
                "10.3"
            ]
          },
          "cost_per_item_original": {
            "type": ["string", "null"],
            "title": "Pre-adjustment per-item cost",
            "examples": [
                "10.3"
            ]
          },
          "items": {
            "type": ["integer", "null"],
            "title": "number of items for per_item costs",
            "examples": [
              10
            ]
          },
          "items_original": {
            "type": ["integer", "null"],
            "title": "pre-adjustments figure of items for per_item costs",
            "examples": [
              10
            ]
          },
          "period": {
            "type": ["integer", "null"],
            "title": "duration in minutes for per_hour costs",
            "examples": [
              180
            ]
          },
          "period_original": {
            "type": ["integer", "null"],
            "title": "pre-adjustment duration in minutes for per_hour costs",
            "examples": [
              180
            ]
          }
        },
        "examples": [
          [
            {
              "id": "fa985737-8172-471a-97ea-f2c87c67111b",
              "name": "A cost",
              "description": "Lorem ipsum",
              "unit_type": "per_hour",
              "cost_per_hour": "10.3",
              "cost_per_item": null,
              "items": null,
              "period": 180
            }
          ]
        ]
      },
      "examples": [
        [
          {
            "id": "fa985737-8172-471a-97ea-f2c87c67111b",
            "name": "A cost",
            "description": "Lorem ipsum",
            "unit_type": "per_hour",
            "cost_per_hour": "10.3",
            "cost_per_item": null,
            "items": null,
            "period": 180
          }
        ]
      ]
    },
    "assessment_comment": {
      "type": ["string", "null"],
      "title": "Assessment comment",
      "examples": [
        "I don't like this"
      ]
    },
    "client_detained": {
      "type": ["boolean", "null"],
      "title": "Whether the client is detained - not relevant for prison law applications",
      "examples": [
        true
      ]
    },
    "court_type": {
      "type": ["string", "null"],
      "title": "type court - not relevant for prison law applications",
      "enum": ["central_criminal_court", "magistrates_court", "crown_court", null],
      "examples": [
        "central_criminal_court"
      ]
    },
    "created_at": {
      "type": "string",
      "title": "Datetime representation of when the application was first started",
      "examples": [
        "2024-08-23T09:27:58.613Z"
      ]
    },
    "custom_main_offence_name": {
      "type": ["string", "null"],
      "title": "name the main offence if the main_offence_id is `other`",
      "examples": [
        "Jaywalking"
      ]
    },
    "custom_prison_name": {
        "type": ["string", "null"],
        "title": "name the prison if the prison_id is `other`",
        "examples": [
            "HMP London"
        ]
