{
  "$id": "https://submit-crime-forms.service.justice.gov.uk/schemas/prior_authority/v1.json",
  "type": "object",
  "title": "Prior Authority Application payload",
  "required": [
    "additional_costs",
    "client_detained",
    "court_type",
    "created_at",
    "custom_main_offence_name",
    "custom_prison_name",
    "custom_service_name",
    "defendant",
    "firm_office",
    "further_information",
    "incorrect_information",
    "laa_reference",
    "main_offence_id",
    "next_hearing",
    "next_hearing_date",
    "no_alternative_quote_reason",
    "office_code",
    "plea",
    "prior_authority_granted",
    "prison_id",
    "prison_law",
    "provider",
    "psychiatric_liaison",
    "psychiatric_liaison_reason_not",
    "quotes",
    "reason_why",
    "rep_order_date",
    "service_type",
    "solicitor",
    "status",
    "subject_to_poca",
    "supporting_documents",
    "ufn",
    "updated_at",
    "youth_court"
